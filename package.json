{
  "name": "upgrade-dependents",
  "version": "1.1.0",
  "description": "A utility to upgrade package dependents",
  "main": "./index.js",
  "bin": {
    "upgrade-dependents": "./bin/upgrade-dependents.js"
  },
  "scripts": {
    "unit": "TS_NODE_FILES=true tap -J -R=spec \"./test/**/*.spec.js\"",
    "test": "npm run unit -- --cov",
    "lint": "tslint --project .",
    "clean": "rimraf ./lib",
    "build": "tsc",
    "report": "nyc report --reporter=lcov",
    "ci": "npm run build && npm run test && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morrisallison/upgrade-dependents.git"
  },
  "keywords": [
    "cli",
    "dependencies",
    "lerna",
    "monorepo",
    "package",
    "semantic-release",
    "semver",
    "utility",
    "versioning",
    "workspace",
    "yarn"
  ],
  "author": "Morris Allison III <author@morris.xyz> (http://morris.xyz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morrisallison/upgrade-dependents/issues"
  },
  "homepage": "https://github.com/morrisallison/upgrade-dependents#readme",
  "engines": {
    "node": "^7.6.0"
  },
  "dependencies": {
    "find-yarn-workspace-root": "^1.2.1",
    "get-monorepo-packages": "^1.2.0",
    "json-file-plus": "^3.3.1",
    "p-limit": "^2.3.0",
    "read-pkg": "^5.2.0",
    "semver": "^7.3.2",
    "semver-utils": "^1.1.4",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/json-file-plus": "^3.3.2",
    "@types/node": "^14.0.9",
    "@types/p-limit": "^2.2.0",
    "@types/read-pkg": "^5.1.0",
    "@types/semver": "^7.2.0",
    "@types/yargs": "^15.0.5",
    "expect": "^26.0.1",
    "prettier": "1.13.6",
    "rimraf": "^3.0.2",
    "tap": "^14.10.7",
    "testdouble": "^3.16.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "files": [
    "bin",
    "lib",
    "index.js",
    "semantic-release.js"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "test/**/*"
    ]
  }
}
